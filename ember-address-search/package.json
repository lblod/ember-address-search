{
  "name": "@lblod/ember-address-search",
  "version": "0.1.0",
  "description": "Addon that provides a service to search for addresses",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lblod/ember-address-search.git"
  },
  "license": "MIT",
  "author": "redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepack": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo \"A v2 addon does not have tests, run tests in test-app\""
  },
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@embroider/addon-shim": "^1.9.0",
    "decorator-transforms": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/runtime": "^7.26.0",
    "@embroider/addon-dev": "^7.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "babel-plugin-ember-template-compilation": "^2.3.0",
    "concurrently": "^9.1.2",
    "ember-template-lint": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.2",
    "prettier": "^3.4.2",
    "rollup": "^4.30.1",
    "rollup-plugin-copy": "^3.5.0"
  },
  "peerDependencies": {
    "ember-source": ">= 4.12.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "app-js": {
      "./services/address-register.js": "./dist/_app_/services/address-register.js"
    },
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "msw": {
    "workerDirectory": "public"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "dist"
  ]
}
